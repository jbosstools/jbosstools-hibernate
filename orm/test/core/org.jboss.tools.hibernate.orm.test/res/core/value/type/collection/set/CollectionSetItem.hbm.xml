<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="core.value.type.collection.set">
  <class name="CollectionSetItem" table="COLLECTION_SET_ITEMS" schema="dbo">
  	<id name="id" column="ID" type="integer"/>
  	<property name="collectionSetId" column="COLLECTION_SET_ID" type="integer"/>
  	<property name="item" column="COLLECTION_SET_ITEM" type="character"/>
  </class>
</hibernate-mapping>